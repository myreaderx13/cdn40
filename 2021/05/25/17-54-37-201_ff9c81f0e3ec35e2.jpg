e":"GettyImages-1027656194.jpg?resize=64,64","width":64,"height":64,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2019\/05\/GettyImages-1027656194.jpg?w=64&h=64&crop=1"},"guest-author-96":{"file":"GettyImages-1027656194.jpg?resize=96,96","width":96,"height":96,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2019\/05\/GettyImages-1027656194.jpg?w=96&h=96&crop=1"},"guest-author-128":{"file":"GettyImages-1027656194.jpg?resize=128,128","width":128,"height":128,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2019\/05\/GettyImages-1027656194.jpg?w=128&h=128&crop=1"},"concierge-thumb":{"file":"GettyImages-1027656194.jpg?resize=50,33","width":50,"height":33,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2019\/05\/GettyImages-1027656194.jpg?w=50"},"full":{"file":"GettyImages-1027656194.jpg","width":1024,"height":676,"mime_type":"image\/jpeg","source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2019\/05\/GettyImages-1027656194.jpg"}},"image_meta":{"aperture":"0","credit":"Getty Images","camera":"","caption":"ANKARA, TURKEY - SEPTEMBER 06 : Logo of 'Amazon' online shopping website is seen inside a circle held by a person in front of a screen in Ankara, Turkey on September 06, 2018.  (Photo by Aytug Can Sencar\/Anadolu Agency\/Getty Images)","created_timestamp":"0","copyright":"2018 Anadolu Agency","focal_length":"0","iso":"0","shutter_speed":"0","title":"'Amazon' online shopping website","orientation":"0","keywords":["Asia,Internet,Ankara,horizontal,technology,shopping,online,2018,"]},"filesize":6197392},"source_url":"https:\/\/techcrunch.com\/wp-content\/uploads\/2019\/05\/GettyImages-1027656194.jpg","_links":{"self":[{"href":"https:\/\/techcrunch.com\/wp-json\/wp\/v2\/media\/1828866"}],"collection":[{"href":"https:\/\/techcrunch.com\/wp-json\/wp\/v2\/media"}],"about":[{"href":"https:\/\/techcrunch.com\/wp-json\